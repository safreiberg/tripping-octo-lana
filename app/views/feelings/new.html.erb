<h2>new feeling item</h2>
<%= simple_form_for(@feeling, :html => {:class => 'form-vertical' }) do |f| %>
  <%= f.error_notification %>
  <%= f.input :target_type, :autofocus => true, :required => true %> 
  <%= f.input :value, :required => true %> 
  <%= f.input :location, :required => true %> 
  <%= f.input :time, :required => true %>
  <%= f.button :submit, 'submit', :class => 'btn-primary' %>
<% end %>
